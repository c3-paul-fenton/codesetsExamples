/*
* CodeAlias.c3typ
*/
entity type CodeAlias schema name 'CODEALIAS' {

   // The {@link MeasurementCode} that measurements are recorded with by clients
   measurementCode: !MeasurementCode

   // Cerners Insight Task label to separate alias mappings by use-case
   insightTask: string
   
   // The alias mapping for this {@link MeasurementCode} / insightTask combination
   alias: string

   // The version associated with this mapping
   aliasVersion: string

}