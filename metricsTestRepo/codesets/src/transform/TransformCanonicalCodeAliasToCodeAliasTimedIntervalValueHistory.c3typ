type TransformCanonicalCodeAliasToCodeAliasTimedIntervalValueHistory mixes CodeAliasTimedIntervalValueHistory transforms CanonicalCodeAlias {
    id: ~ expression "concat(measurementCode, '_', start)"
    parent: ~ expression { id: 'measurementCode' }
    start: ~ expression "dateTime(start,'y-M-d H:m:s')"
    end: ~ expression "dateTime(end,'y-M-d H:m:s')"
    value: ~ expression "alias"
}