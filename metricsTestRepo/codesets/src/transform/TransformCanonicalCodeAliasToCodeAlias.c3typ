type TransformCanonicalCodeAliasToCodeAlias mixes CodeAlias transforms CanonicalCodeAlias {
    id: ~ expression 'concat(measurementCode,"_",insightTask,"_",aliasVersion)'
    measurementCode: ~ expression { id: "measurementCode" }
    insightTask: ~ expression 'insightTask'
    alias: ~ expression 'alias'
    aliasVersion: ~ expression 'aliasVersion'
}